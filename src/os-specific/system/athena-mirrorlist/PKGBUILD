pkgname=athena-mirrorlist
pkgver=1.0.2
pkgrel=1
pkgdesc='Athena OS mirrorlist.'
arch=('any')
license=('custom:unknown')
backup=('etc/pacman.d/athena-mirrorlist')
source=("athena-mirrorlist")
sha512sums=('632da9957e8226e8949542efb51135b4ba8a2360c50a4b9deddd338877ff9ef95df7cb53e5c3eb9e69b839303f17447fd365369ddb9882cb8a59c6db0dcce957')

package() {
  mkdir -p "$pkgdir/etc/pacman.d"

  install -Dm 644 -t "$pkgdir/etc/pacman.d/" $pkgname
}

