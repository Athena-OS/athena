[Desktop Entry]
Name=ssldump
Comment=Dump SSL traffic on a network
Encoding=UTF-8
Exec=shell-rocket "if command -v ssldump &> /dev/null;then ssldump -h;$SHELL;else echo \\"ssldump is not installed. I'm retrieving it for you...\\";sudo pacman -S ssldump;ssldump -h;$SHELL;fi;"
Icon=kali-ssldump
StartupNotify=false
Terminal=false
Type=Application
Categories=01-13-ssl-analysis;
X-Athena-Package=ssldump
