Encoding=UTF-8
Exec=shell-rocket "if command -v dcfldd &> /dev/null;then dcfldd --help;$SHELL;else echo \\"dcfldd is not installed. I'm retrieving it for you...\\";sudo pacman -S dcfldd;dcfldd --help;$SHELL;fi;"
