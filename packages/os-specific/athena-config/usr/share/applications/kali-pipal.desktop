[Desktop Entry]
Name=pipal
Encoding=UTF-8
Exec=shell-rocket "if command -v pipal &> /dev/null;then pipal -h;$SHELL;else echo \\"pipal is not installed. I'm retrieving it for you...\\";sudo pacman -S pipal;pipal -h;$SHELL;fi;"
Icon=kali-pipal
StartupNotify=false
Terminal=false
Type=Application
Categories=12-reporting;
X-Athena-Package=pipal
