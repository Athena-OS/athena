{
    "main": {
        "enable_logging": false,
        "enable_websocket": false
    },
    "module": {
        "messages": [
            {
                "name": {
                    "external": true
                }
            }
        ],
        "channelName": "ptk_module",
        "options": {}
    },
    "proxy": {
        "max_tabs": 5,
        "max_requests_per_tab": 1000
    },
    "rbuilder": {

    },
    "rattacker": {
        "max_requests": 1000,
        "blacklist": [
            "stylesheet",
            "script",
            "image",
            "font"
        ],
        "enable_external_integration": false,
        "rattacker_key": ""
    },
    "decoder": {
        "settings": {}
    },
    "privacy": {
        "enable_personal_data": false,
        "enable_cookie": true,
        "enable_anonymous_data": false
    },
    "recorder": {
        "recorderFile": "/ptk/content/recorder.js",
        "trackerFile": "/ptk/content/tracker.js",
        "popupFile": "/ptk/content/popup.js",
        "replayerFile": "/ptk/content/replayer.js",
        "set_window_size": true,
        "window_height": 768,
        "window_width": 1024,
        "icons": "/ptk/browser/assets/icons",
        "double_click": false
    },
    "macro": {
        "format": "xml",
        "min_duration": 3000,
        "enable_extra_delay": false,
        "element_path": "id",
        "event_type": "javascript",
        "enable_regex": false,
        "validate_regex": "(sign|log)[ -]?(out|off)"
    },
    "traffic": {
        "format": "har"
    }
}