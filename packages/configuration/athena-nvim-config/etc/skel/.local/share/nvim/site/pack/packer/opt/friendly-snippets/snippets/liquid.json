    "Shopify Schema": {
        "prefix": "_schema",
        "body": [
            "{% schema %}",
            "\t{",
            "\t\t\"name\": \"$1\",",
            "\t\t\"class\": \"$2\",",
            "\t\t\"settings\": [",
            "\t\t$3",
            "\t\t]",
            "\t}",
            "{% endschema %}",
            ""
        ]
    },
